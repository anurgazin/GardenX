<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{details.name}}</title>
  </head>
  <body>
      {{> sideMenu}}
    <img
      src="{{details.photo}}"
      alt="{{details.name}}"
      height="400px"
      width="auto"
    />
    <h2>Tips: </h2>
    {{#each tips}}
        <p>{{this}}</p>
    {{/each}}
    <h2>Schedule: </h2>
    <p>{{details.schedule}}</p>
  </body>
</html>